{
  "name": "<%= appName %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsWatch": "tsc -w",
    "browserifyWatch": "gulp dev --gulpfile bin/gulpfile.js",
    "start": "concurrently --kill-others 'npm run tsWatch' 'npm run browserifyWatch'",
    "build": "gulp prod --gulpfile bin/gulpfile.js",
    "doc": "gulp doc --gulpfile bin/gulpfile.js",
    "typingsDependencies": "typings install dt~react --global; typings install dt~react-dom --global; typings install dt~react-addons-create-fragment --global;"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/<%= userName %>/<%= appName %>.git"
  },
  "keywords": [
    "slushgenerator"
  ],
  "author": "<%= authorName %> <<%= authorEmail %>>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= appName %>/issues"
  },
  "homepage": "https://github.com/<%= userName %>/<%= appName %>#readme",
  "dependencies": {
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "browser-sync": "^2.17.3",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.4",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
