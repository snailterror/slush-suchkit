{
  "name": "<%= appName %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsWatch": "tsc -w",
    "browserifyWatch": "gulp dev --gulpfile bin/gulpfile.js",
    "start": "concurrently --kill-others 'npm run tsWatch' 'npm run browserifyWatch'",
    "build": "gulp prod --gulpfile bin/gulpfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/<%= userName %>/<%= appName %>.git"
  },
  "keywords": [
    "slushgenerator"
  ],
  "author": "<%= authorName %> <<%= authorEmail %>>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= appName %>/issues"
  },
  "homepage": "https://github.com/<%= userName %>/<%= appName %>#readme",
  "devDependencies": {
    "babelify": "^7.2.0",
    "babel-cli": "^6.1.4",
    "babel-core": "^6.1.2",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-stage-0": "^6.1.18",
    "browserify": "^10.2.4",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "concurrently": "^2.2.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "run-sequence": "^1.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.2"
  },
  "dependencies" : {
  }
}
